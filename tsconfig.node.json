{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023"],
    "module": "NodeNext",
    "moduleResolution": "nodenext",
    "skipLibCheck": true,
    "strict": true,
    "outDir": ".vercel/output",
    "composite": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  },
  "include": ["api/**/*.ts", "vite.config.ts", "eslint.config.js"]
}
